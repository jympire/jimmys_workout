<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div>
      <% @posts.each do |post| %>
        <div class="panel panel-default post">
          <div class="container">
            <p>
              <%= post.created_at.strftime("%B %d, %Y") %>
            </p>
            <p><%= post.content %></p>
          </div> 
        </div>
      <% end %>     
    </div>  
  </div>  
</div>